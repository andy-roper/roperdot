# STATUS - STALE
regexp=^(\S+)\s.*(STALE)$
colours=default,"\033[38;5;203m","\033[01;38;5;131m"
-
# STATUS - FAILED
regexp=^(\S+)\s.*(FAILED)$
colours=default,"\033[38;5;139m","\033[01;38;5;96m"
-
# Status - REACHABLE
regexp=^(\S+)\s.*(REACHABLE)$
colours=default,"\033[38;5;143m","\033[38;5;101m"
-
# Status - DELAY
regexp=^(\S+)\s.*(DELAY)$
colours=default,"\033[38;5;179m","\033[38;5;173m"
-
# DEV
regexp=dev\s(\S+)
colours=default,"\033[38;5;66m"
