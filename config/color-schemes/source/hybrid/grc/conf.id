# Groups
regexp=(\d+)\((\w+)\)
colours=unchanged,"\033[38;5;173m","\033[01;38;5;173m"
-
# User
regexp=uid.(\d+)\((\w+)\)
colours=unchanged,"\033[38;5;101m","\033[01;38;5;101m"
-
# SELinux
regexp=(\w+_u):(\w+_r):(\w+_t):([\w\-.:]+)
colours=unchanged,"\033[38;5;101m","\033[38;5;173m","\033[38;5;66m","\033[38;5;96m"
