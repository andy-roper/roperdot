# Main Nodes
regexp=^(\S[^:]+):\s?(.*)?$
colours=default,"\033[38;5;73m"
======
# Sub Nodes
regexp=^\s([^:]+):\s?(.*)?$
colours=default,"\033[38;5;99m"
======
# Warning
regexp=WARNING:\s(.+)$
colours="\033[01;38;5;185m","\033[38;5;185m"
======
# devicemapper
regexp=devicemapper$
colours="\033[38;5;198m"
-
# loop-lvm
regexp=: (/var/lib/docker/devicemapper/devicemapper/(?:meta)?data)
colours=default,"\033[38;5;198m"
