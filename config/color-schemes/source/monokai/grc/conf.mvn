# mvn grc colorizer configuration
# [INFO]
regexp=^\[INFO\]
colours="\033[01;38;5;m"
count=more
==============
# [WARNING]
regexp=^\[WARNING\]
colours="\033[01;38;5;185m"
count=more
==============
# [ERROR]
regexp=^\[ERROR\]
colours="\033[01;38;5;161m"
count=more
==============
# BUILD FAILURE
regexp=BUILD FAILURE
colours="\033[01;38;5;161m"
count=more
==============
# [debug]
regexp=^\[debug\]
colours="\033[38;5;99m"
count=more
==============
# lines [INFO] ----
regexp=\s[-]{6,}
colours="\033[38;5;161m"
count=more
==============
# lines ^----
regexp=^[-]{6,}
colours="\033[38;5;185m"
count=more
==============
# lines  T E S T S
regexp=^ T E S T S
colours="\033[38;5;185m"
count=more
==============
# lines ^Tests run:
regexp=^Tests run: ([\d]+)
colours="\033[38;5;185m","\033[38;5;106m"
count=more
==============
# lines ^Tests run: Failures/Errors/Skipped
regexp=(Failures|Errors|Skipped):\s([\d]+)
colours="\033[38;5;m","\033[38;5;185m","\033[01;38;5;161m"
count=more
==============
# lines ^Tests run: Failures/Errors/Skipped
regexp=(Failures|Errors|Skipped):\s(0)\D?
colours="\033[38;5;m","\033[38;5;185m","\033[38;5;106m"
count=more
==============
# summary
regexp=\s(Total time: )(.*)$
colours="\033[38;5;m","\033[38;5;m","\033[01;38;5;185m"
count=more
==============
# summary
regexp=\s(Finished at: )(.*)$
colours="\033[38;5;m","\033[38;5;m","\033[01;38;5;185m"
count=more
==============
# BUILD SUCCESSFUL
regexp=\s(BUILD SUCCESSFUL)
colours="\033[38;5;m","\033[01;38;5;106m"
count=more
==============
# Building projectName
regexp=^(\[INFO\])( Building )(.*)$
colours="\033[38;5;m","\033[01;38;5;m","\033[38;5;m","\033[01;38;5;145m"
count=more
==============
# reactor summary
regexp=([.]{3,} )(SUCCESS)( \[)([^\]]*)(])
colours="\033[38;5;m","\033[38;5;m","\033[38;5;106m","\033[38;5;m","\033[38;5;185m","\033[38;5;m"
count=more
==============
# reactor summary
regexp=([.]{3,} )(FAILURE)( \[)([^\]]*)(])
colours="\033[38;5;m","\033[38;5;m","\033[38;5;161m","\033[38;5;m","\033[38;5;161m","\033[38;5;m"
count=more
==============
# reactor summary
regexp=([.]{3,} )(SKIPPED)
colours="\033[38;5;m","\033[38;5;m","\033[01;38;5;185m","\033[38;5;m"
count=more


