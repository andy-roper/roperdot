# Groups
regexp=(\d+)\((\w+)\)
colours=unchanged,"\033[38;5;221m","\033[01;38;5;221m"
-
# User
regexp=uid.(\d+)\((\w+)\)
colours=unchanged,"\033[38;5;108m","\033[01;38;5;108m"
-
# SELinux
regexp=(\w+_u):(\w+_r):(\w+_t):([\w\-.:]+)
colours=unchanged,"\033[38;5;108m","\033[38;5;221m","\033[38;5;80m","\033[38;5;176m"
