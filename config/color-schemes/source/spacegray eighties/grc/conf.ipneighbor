# STATUS - STALE
regexp=^(\S+)\s.*(STALE)$
colours=default,"\033[38;5;210m","\033[01;38;5;210m"
-
# STATUS - FAILED
regexp=^(\S+)\s.*(FAILED)$
colours=default,"\033[38;5;213m","\033[01;38;5;176m"
-
# Status - REACHABLE
regexp=^(\S+)\s.*(REACHABLE)$
colours=default,"\033[38;5;151m","\033[38;5;108m"
-
# Status - DELAY
regexp=^(\S+)\s.*(DELAY)$
colours=default,"\033[38;5;221m","\033[38;5;221m"
-
# DEV
regexp=dev\s(\S+)
colours=default,"\033[38;5;80m"
