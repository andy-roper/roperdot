# Main Nodes
regexp=^(\S[^:]+):\s?(.*)?$
colours=default,"\033[38;5;116m"
======
# Sub Nodes
regexp=^\s([^:]+):\s?(.*)?$
colours=default,"\033[38;5;139m"
======
# Warning
regexp=WARNING:\s(.+)$
colours="\033[01;38;5;173m","\033[38;5;173m"
======
# devicemapper
regexp=devicemapper$
colours="\033[38;5;210m"
-
# loop-lvm
regexp=: (/var/lib/docker/devicemapper/devicemapper/(?:meta)?data)
colours=default,"\033[38;5;210m"
