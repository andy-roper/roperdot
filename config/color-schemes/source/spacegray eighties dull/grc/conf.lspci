# Address
regexp=^(..):(..).(.)
colours=default,"\033[38;5;114m","\033[38;5;221m","\033[38;5;131m"
-
# Ethernet
regexp=(Ethernet controller|Network controller)
colours="\033[38;5;116m"
-
# Wireless
regexp=Wireless
colours="\033[38;5;80m"
-
# SATA
regexp=SATA controller
colours="\033[38;5;144m"
-
# Audio
regexp=Audio device
colours="\033[38;5;109m"
