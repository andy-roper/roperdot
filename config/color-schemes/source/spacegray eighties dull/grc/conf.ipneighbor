# STATUS - STALE
regexp=^(\S+)\s.*(STALE)$
colours=default,"\033[38;5;210m","\033[01;38;5;131m"
-
# STATUS - FAILED
regexp=^(\S+)\s.*(FAILED)$
colours=default,"\033[38;5;176m","\033[01;38;5;139m"
-
# Status - REACHABLE
regexp=^(\S+)\s.*(REACHABLE)$
colours=default,"\033[38;5;114m","\033[38;5;144m"
-
# Status - DELAY
regexp=^(\S+)\s.*(DELAY)$
colours=default,"\033[38;5;221m","\033[38;5;173m"
-
# DEV
regexp=dev\s(\S+)
colours=default,"\033[38;5;116m"
