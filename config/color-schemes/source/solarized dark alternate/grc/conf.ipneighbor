# STATUS - STALE
regexp=^(\S+)\s.*(STALE)$
colours=default,"\033[38;5;160m","\033[01;38;5;167m"
-
# STATUS - FAILED
regexp=^(\S+)\s.*(FAILED)$
colours=default,"\033[38;5;61m","\033[01;38;5;61m"
-
# Status - REACHABLE
regexp=^(\S+)\s.*(REACHABLE)$
colours=default,"\033[38;5;64m","\033[38;5;100m"
-
# Status - DELAY
regexp=^(\S+)\s.*(DELAY)$
colours=default,"\033[38;5;136m","\033[38;5;136m"
-
# DEV
regexp=dev\s(\S+)
colours=default,"\033[38;5;36m"
