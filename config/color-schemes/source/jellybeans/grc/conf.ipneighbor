# STATUS - STALE
regexp=^(\S+)\s.*(STALE)$
colours=default,"\033[38;5;217m","\033[01;38;5;174m"
-
# STATUS - FAILED
regexp=^(\S+)\s.*(FAILED)$
colours=default,"\033[38;5;225m","\033[01;38;5;189m"
-
# Status - REACHABLE
regexp=^(\S+)\s.*(REACHABLE)$
colours=default,"\033[38;5;187m","\033[38;5;144m"
-
# Status - DELAY
regexp=^(\S+)\s.*(DELAY)$
colours=default,"\033[38;5;223m","\033[38;5;222m"
-
# DEV
regexp=dev\s(\S+)
colours=default,"\033[38;5;37m"
