# ant grc colorizer configuration
# BUILD FAILED
regexp=BUILD FAILED
colours="\033[01;38;5;174m"
count=more
==============
# BUILD SUCCESSFUL
regexp=^BUILD SUCCESSFUL
colours="\033[01;38;5;144m"
count=more
==============
# Total time
regexp=^(Total time: )([\d]+.*)$
colours="\033[38;5;222m","\033[01;38;5;189m"
count=more
===============
# some error
regexp=[\d]+ error[s]?
colours="\033[38;5;174m"
count=more
==============
# some warning
regexp=[\d]+ warning[s]?
colours="\033[38;5;222m"
count=more
===============
# some error
regexp=[Ee]rror:
colours="\033[38;5;174m"
count=more
==============
# some warning
regexp=[Ww]arning:
colours="\033[38;5;222m"
count=more
==============
# project name
regexp=^[^:\s]+:$
colours="\033[38;5;144m"
count=more
==============
# products
regexp=[^/]+\.[ewrj]ar$
colours="\033[38;5;152m"
count=more

