# Normal
regexp=[aAcCdDeijsStTu]
colours="\033[38;5;37m"
-
# User Namespace
regexp=[iadA]
colours="\033[38;5;187m"
-
# Read Only
regexp=[EhINXZ]
colours="\033[01;38;5;174m"
-
# Separators
regexp=[\-]
