# Address
regexp=^(..):(..).(.)
colours=default,"\033[38;5;187m","\033[38;5;223m","\033[38;5;174m"
-
# Ethernet
regexp=(Ethernet controller|Network controller)
colours="\033[38;5;37m"
-
# Wireless
regexp=Wireless
colours="\033[38;5;43m"
-
# SATA
regexp=SATA controller
colours="\033[38;5;144m"
-
# Audio
regexp=Audio device
colours="\033[38;5;152m"
