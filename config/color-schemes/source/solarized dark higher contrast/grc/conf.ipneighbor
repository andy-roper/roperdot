# STATUS - STALE
regexp=^(\S+)\s.*(STALE)$
colours=default,"\033[38;5;203m","\033[01;38;5;167m"
-
# STATUS - FAILED
regexp=^(\S+)\s.*(FAILED)$
colours=default,"\033[38;5;169m","\033[01;38;5;168m"
-
# Status - REACHABLE
regexp=^(\S+)\s.*(REACHABLE)$
colours=default,"\033[38;5;84m","\033[38;5;114m"
-
# Status - DELAY
regexp=^(\S+)\s.*(DELAY)$
colours=default,"\033[38;5;137m","\033[38;5;136m"
-
# DEV
regexp=dev\s(\S+)
colours=default,"\033[38;5;36m"
