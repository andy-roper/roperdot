# Address
regexp=^(..):(..).(.)
colours=default,"\033[38;5;149m","\033[38;5;222m","\033[38;5;203m"
-
# Ethernet
regexp=(Ethernet controller|Network controller)
colours="\033[38;5;37m"
-
# Wireless
regexp=Wireless
colours="\033[38;5;87m"
-
# SATA
regexp=SATA controller
colours="\033[38;5;71m"
-
# Audio
regexp=Audio device
colours="\033[38;5;67m"
