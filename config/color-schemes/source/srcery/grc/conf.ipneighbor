# STATUS - STALE
regexp=^(\S+)\s.*(STALE)$
colours=default,"\033[38;5;203m","\033[01;38;5;203m"
-
# STATUS - FAILED
regexp=^(\S+)\s.*(FAILED)$
colours=default,"\033[38;5;168m","\033[01;38;5;168m"
-
# Status - REACHABLE
regexp=^(\S+)\s.*(REACHABLE)$
colours=default,"\033[38;5;149m","\033[38;5;71m"
-
# Status - DELAY
regexp=^(\S+)\s.*(DELAY)$
colours=default,"\033[38;5;222m","\033[38;5;215m"
-
# DEV
regexp=dev\s(\S+)
colours=default,"\033[38;5;37m"
