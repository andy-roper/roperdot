# Main Nodes
regexp=^(\S[^:]+):\s?(.*)?$
colours=default,"\033[38;5;37m"
======
# Sub Nodes
regexp=^\s([^:]+):\s?(.*)?$
colours=default,"\033[38;5;168m"
======
# Warning
regexp=WARNING:\s(.+)$
colours="\033[01;38;5;215m","\033[38;5;215m"
======
# devicemapper
regexp=devicemapper$
colours="\033[38;5;203m"
-
# loop-lvm
regexp=: (/var/lib/docker/devicemapper/devicemapper/(?:meta)?data)
colours=default,"\033[38;5;203m"
