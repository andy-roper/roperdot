# STATUS - STALE
regexp=^(\S+)\s.*(STALE)$
colours=default,"\033[38;5;203m","\033[01;38;5;160m"
-
# STATUS - FAILED
regexp=^(\S+)\s.*(FAILED)$
colours=default,"\033[38;5;174m","\033[01;38;5;132m"
-
# Status - REACHABLE
regexp=^(\S+)\s.*(REACHABLE)$
colours=default,"\033[38;5;142m","\033[38;5;100m"
-
# Status - DELAY
regexp=^(\S+)\s.*(DELAY)$
colours=default,"\033[38;5;214m","\033[38;5;172m"
-
# DEV
regexp=dev\s(\S+)
colours=default,"\033[38;5;71m"
