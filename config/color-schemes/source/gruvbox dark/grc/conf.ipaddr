# IP4
regexp=inet\s([^\/]+)\/(\d+)
colours=default,"\033[01;38;5;172m","\033[01;38;5;132m"
=====
# broadcast
regexp=brd\s([^}s]+)
colours=default,"\033[38;5;m"
=====
# IP6
regexp=inet6\s([^\/]+)\/(\d+)
colours=default,"\033[38;5;172m","\033[38;5;132m"
=====
# MAC
regexp=link\/ether\s(\S+)\s
colours=default,"\033[38;5;132m"
=====
# State UP
regexp=(\d+):\s(\S+):.+state (UP)
colours=default,"\033[38;5;187m","\033[01;38;5;100m","\033[01;38;5;100m"
=====
# State DOWN
regexp=(\d+):\s(\S+):.+state (DOWN)
colours=default,"\033[38;5;187m","\033[01;38;5;160m","\033[01;38;5;160m"
=====
# State UNKNOWN
regexp=(\d+):\s(\S+):.+state (UNKNOWN)
colours=default,"\033[38;5;187m","\033[01;38;5;71m","\033[38;5;71m"
=====
# State DORMANT
regexp=(\d+):\s(\S+):.+state (DORMANT)
colours=default,"\033[38;5;187m","\033[01;38;5;71m","\033[38;5;71m"
=====
# < >
regexp=\s<([^>]+)>
colours=default,"\033[38;5;71m"
=====
# Master dev
regexp=\smaster\s(\S+)\s
colours=default,"\033[38;5;138;48;5;66m"
=====
# on
regexp=\son\s
colours="\033[38;5;100m"
=====
# off
regexp=\soff\s
colours="\033[38;5;160m"
=====
# link/none
regexp=link\/(none)
colours=unchanged,"\033[38;5;160m"
