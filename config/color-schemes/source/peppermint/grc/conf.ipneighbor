# STATUS - STALE
regexp=^(\S+)\s.*(STALE)$
colours=default,"\033[38;5;175m","\033[01;38;5;204m"
-
# STATUS - FAILED
regexp=^(\S+)\s.*(FAILED)$
colours=default,"\033[38;5;219m","\033[01;38;5;176m"
-
# Status - REACHABLE
regexp=^(\S+)\s.*(REACHABLE)$
colours=default,"\033[38;5;151m","\033[38;5;114m"
-
# Status - DELAY
regexp=^(\S+)\s.*(DELAY)$
colours=default,"\033[38;5;186m","\033[38;5;179m"
-
# DEV
regexp=dev\s(\S+)
colours=default,"\033[38;5;109m"
