# Address
regexp=^(..):(..).(.)
colours=default,"\033[38;5;100m","\033[38;5;136m","\033[38;5;166m"
-
# Ethernet
regexp=(Ethernet controller|Network controller)
colours="\033[38;5;108m"
-
# Wireless
regexp=Wireless
colours="\033[38;5;65m"
-
# SATA
regexp=SATA controller
colours="\033[38;5;142m"
-
# Audio
regexp=Audio device
colours="\033[38;5;66m"
