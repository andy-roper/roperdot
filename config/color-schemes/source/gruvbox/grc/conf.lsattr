# Normal
regexp=[aAcCdDeijsStTu]
colours="\033[38;5;108m"
-
# User Namespace
regexp=[iadA]
colours="\033[38;5;100m"
-
# Read Only
regexp=[EhINXZ]
colours="\033[01;38;5;166m"
-
# Separators
regexp=[\-]
