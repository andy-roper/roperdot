# Main Nodes
regexp=^(\S[^:]+):\s?(.*)?$
colours=default,"\033[38;5;108m"
======
# Sub Nodes
regexp=^\s([^:]+):\s?(.*)?$
colours=default,"\033[38;5;138m"
======
# Warning
regexp=WARNING:\s(.+)$
colours="\033[01;38;5;178m","\033[38;5;178m"
======
# devicemapper
regexp=devicemapper$
colours="\033[38;5;124m"
-
# loop-lvm
regexp=: (/var/lib/docker/devicemapper/devicemapper/(?:meta)?data)
colours=default,"\033[38;5;124m"
