# /full/path
regexp=(\s|^)/[-\w\d.]+/[-\w\d./]+
colours="\033[01;38;5;253m"
=======
# CAPS LINE
regexp=^[A-Z\s%]*([A-Z]{3})[A-Z\s%]*$
colours=underline
=======
# Capd Line
regexp=^([A-Z][-a-z0-9]+(\s+|$)){3,}$
colours=underline
=======
# VMSTAT
regexp=(\s|^)\d+([.,]\d+)?(?=[\s,]|$)
colours="\033[38;5;151m"
=======
# PID
regexp=^[a-zA-Z]+\w+\s+(\d+)|^\d\s+\w\s+(?:\w+\s+)?(\d+)|^\s*(\d+)
colours=unchanged,"\033[01;38;5;182m","\033[01;38;5;182m","\033[01;38;5;182m"
=======
# n.n.n
regexp=(\s|^)\d+\.\d+\.\d+(?=[\s,]|$)
colours="\033[01;38;5;115m"
=======
# text:
regexp=^([-a-z0-9]+):\s
colours=unchanged,"\033[38;5;180m"
=======
# -options
regexp=(?<=\s)-[\w\d]+(?=\s|$)
colours="\033[38;5;180m"
=======
# --long-option=
# legacy: regexp=(?<=\s)--[-\w\d]+[\w\d](?==|\s|$)(=?)
regexp=(?<=\s)--[-\w\d]+[\w\d](?==|\s|$)(=|\s)?(?!--)([^\s]*)
colours="\033[38;5;115m","\033[38;5;253m","\033[04;38;5;115m"
=======
# [text]
regexp=\[[-\w\d:/]+\]
colours="\033[38;5;115m"
=======
# root
regexp=root|wheel(?=\s|$)
colours="\033[01;38;5;138m"
=======
# pts
regexp=(\s|^)pts/\d+(?=[^\w\d]|$)
colours="\033[38;5;187m"
=======
# tty
regexp=(\s|^)tty\d+(?=[^\w\d]|$)
colours="\033[38;5;152m"
=======
# Negative NICE (works only in -l)
regexp=^\d\s+\w\s+\w+\s+\d+\s+\d+\s+\d\s+\d+\s+(-\d+)
colours=unchanged,"\033[01;38;5;253;48;5;138m"
=======
# Neutral NICE (works only in -l)
regexp=^\d\s+\w\s+\w+\s+\d+\s+\d+\s+\d\s+\d+\s+(\d+)
colours=unchanged,"\033[38;5;115m"
=======
# Positive NICE (works only in -l)
regexp=^\d\s+\w\s+\w+\s+\d+\s+\d+\s+\d\s+\d+\s+(1\d)
colours=unchanged,"\033[01;38;5;253;48;5;115m"
=======
# Process ZOMBIE
regexp=^\d\s+([zZ])\s
colours=unchanged,"\033[01;38;5;253;48;5;138m"
=======
# Process RS
regexp=^\d\s+([sSrR])\s
colours=unchanged,"\033[38;5;237;48;5;182m"
