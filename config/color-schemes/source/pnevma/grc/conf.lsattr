# Normal
regexp=[aAcCdDeijsStTu]
colours="\033[38;5;115m"
-
# User Namespace
regexp=[iadA]
colours="\033[38;5;151m"
-
# Read Only
regexp=[EhINXZ]
colours="\033[01;38;5;138m"
-
# Separators
regexp=[\-]
colours="\033[38;5;m"
-
# Error
regexp=(lsattr:) \w.*
colours=default,"\033[01;38;5;138m"
-
# Filename
regexp=(\/[-\w\d. ]+)+$
colours="\033[38;5;180m","\033[01;38;5;m"
