# Main Nodes
regexp=^(\S[^:]+):\s?(.*)?$
colours=default,"\033[38;5;115m"
======
# Sub Nodes
regexp=^\s([^:]+):\s?(.*)?$
colours=default,"\033[38;5;182m"
======
# Warning
regexp=WARNING:\s(.+)$
colours="\033[01;38;5;180m","\033[38;5;180m"
======
# devicemapper
regexp=devicemapper$
colours="\033[38;5;174m"
-
# loop-lvm
regexp=: (/var/lib/docker/devicemapper/devicemapper/(?:meta)?data)
colours=default,"\033[38;5;174m"
