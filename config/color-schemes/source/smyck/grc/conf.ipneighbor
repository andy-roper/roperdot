# STATUS - STALE
regexp=^(\S+)\s.*(STALE)$
colours=default,"\033[38;5;181m","\033[01;38;5;167m"
-
# STATUS - FAILED
regexp=^(\S+)\s.*(FAILED)$
colours=default,"\033[38;5;219m","\033[01;38;5;182m"
-
# Status - REACHABLE
regexp=^(\S+)\s.*(REACHABLE)$
colours=default,"\033[38;5;192m","\033[38;5;143m"
-
# Status - DELAY
regexp=^(\S+)\s.*(DELAY)$
colours=default,"\033[38;5;222m","\033[38;5;179m"
-
# DEV
regexp=dev\s(\S+)
colours=default,"\033[38;5;31m"
