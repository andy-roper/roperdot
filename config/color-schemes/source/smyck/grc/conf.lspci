# Address
regexp=^(..):(..).(.)
colours=default,"\033[38;5;192m","\033[38;5;222m","\033[38;5;167m"
-
# Ethernet
regexp=(Ethernet controller|Network controller)
colours="\033[38;5;31m"
-
# Wireless
regexp=Wireless
colours="\033[38;5;116m"
-
# SATA
regexp=SATA controller
colours="\033[38;5;143m"
-
# Audio
regexp=Audio device
colours="\033[38;5;110m"
