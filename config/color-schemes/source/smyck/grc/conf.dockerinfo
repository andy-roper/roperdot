# Main Nodes
regexp=^(\S[^:]+):\s?(.*)?$
colours=default,"\033[38;5;31m"
======
# Sub Nodes
regexp=^\s([^:]+):\s?(.*)?$
colours=default,"\033[38;5;182m"
======
# Warning
regexp=WARNING:\s(.+)$
colours="\033[01;38;5;179m","\033[38;5;179m"
======
# devicemapper
regexp=devicemapper$
colours="\033[38;5;181m"
-
# loop-lvm
regexp=: (/var/lib/docker/devicemapper/devicemapper/(?:meta)?data)
colours=default,"\033[38;5;181m"
