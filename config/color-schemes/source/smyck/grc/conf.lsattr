# Normal
regexp=[aAcCdDeijsStTu]
colours="\033[38;5;31m"
-
# User Namespace
regexp=[iadA]
colours="\033[38;5;192m"
-
# Read Only
regexp=[EhINXZ]
colours="\033[01;38;5;167m"
-
# Separators
regexp=[\-]
