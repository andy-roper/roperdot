# Address
regexp=^(..):(..).(.)
colours=default,"\033[38;5;151m","\033[38;5;187m","\033[38;5;174m"
-
# Ethernet
regexp=(Ethernet controller|Network controller)
colours="\033[38;5;116m"
-
# Wireless
regexp=Wireless
colours="\033[38;5;152m"
-
# SATA
regexp=SATA controller
colours="\033[38;5;114m"
-
# Audio
regexp=Audio device
colours="\033[38;5;104m"
