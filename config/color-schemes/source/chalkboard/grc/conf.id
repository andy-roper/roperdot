# Groups
regexp=(\d+)\((\w+)\)
colours=unchanged,"\033[38;5;186m","\033[01;38;5;186m"
-
# User
regexp=uid.(\d+)\((\w+)\)
colours=unchanged,"\033[38;5;114m","\033[01;38;5;114m"
-
# SELinux
regexp=(\w+_u):(\w+_r):(\w+_t):([\w\-.:]+)
colours=unchanged,"\033[38;5;114m","\033[38;5;186m","\033[38;5;116m","\033[38;5;176m"
